#!/usr/bin/rash
# vim: set ft=yaml:
---

- template:
    src: /Server.cfg
    dest: /blackwake/Server.cfg

- command:
    transfer_pid_1: true
    argv:
      - "xvfb-run"
      - "-a"
      - "wine"
      - "./BlackwakeServer.exe"
      - "-batchmode"
      - "-nographics"
      - "-logfile"
      - "-"
